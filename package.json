  "scripts": {
    "server": "webpack-dev-server --inline --port 3000 --content-base dist/",
    "analyze": "webpack --json | webpack-bundle-size-analyzer",
    "clean": "rimraf dist/**/*",
    "build": "webpack --config config/webpack.dev.js --watch --progress --color",
    "build:prod": "npm run clean && npm install -f && set NODE_ENV=production && npm run test && npm run lint && webpack --config config/webpack.prod.js --progress --color -p",
    "lint": "eslint src/ --fix --color",
    "start": "npm run clean && npm run build",
    "test": "karma start",
    "test:debug": "karma start karma.conf.js --log-level=debug --single-run=False",
    "test:watch": "karma start karma.conf.js --auto-watch=true --single-run=False"
  },
